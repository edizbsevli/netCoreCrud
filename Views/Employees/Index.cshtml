@model List<Deneme.Models.Domain.Employee>

@{

}

<h1>Oyunlar</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Ad</th>
            <th>Aldığın Fiyat</th>
            <th>Aldığın Tarih</th>
            <th>Platform</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Salary</td>
                <td>@employee.DateOfBirth.ToString("dd-MM-yyyy")</td>
                <td>@employee.Department</td>
                <td><a href="Employees/View/@employee.Id">Göster</a></td>
            </tr>
        }
    </tbody>
</table>